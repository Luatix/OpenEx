{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    "coverage",
    "packages",
    "public",
    "builder/dev/build",
    "builder/prod/build",
    "src/static/ext/*",
    "__generated__"
  ],
  "exclude": [
    "src/static/geo/countries.json"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
